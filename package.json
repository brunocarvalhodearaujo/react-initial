{
  "name": "react-initial",
  "version": "1.0.12",
  "description": "react component to make Gmail like text avatars for profile pictures",
  "author": "Bruno Carvalho de Araujo <brunocarvalhodearaujo@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "initial",
    "avatar"
  ],
  "homepage": "https://brunocarvalhodearaujo.github.io/react-initial/",
  "repository": {
    "url": "https://github.com/brunocarvalhodearaujo/react-initial.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/brunocarvalhodearaujo/react-initial/issues"
  },
  "typings": "index.d.ts",
  "main": "lib/index.js",
  "scripts": {
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "test": "jest --no-cache --coverage",
    "prepublish": "babel src --out-dir lib"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.8.7",
    "@babel/preset-flow": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "@types/jest": "25.1.4",
    "babel-loader": "8.1.0",
    "jest": "25.1.0",
    "react-styleguidist": "11.2.0",
    "react-test-renderer": "16.13.0",
    "webpack": "4.43.0"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "jsdom",
    "coverageReporters": [
      "text-summary",
      "lcov"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/index.js",
      "!src/**/*.{test,spec}.js"
    ],
    "testRegex": "test/.*\\.spec\\.js$",
    "coverageThreshold": {
      "global": {
        "statements": 75,
        "branches": 75,
        "functions": 75,
        "lines": 75
      }
    }
  }
}
